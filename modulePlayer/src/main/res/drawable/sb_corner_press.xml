<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/transparent" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="@dimen/dp_5"
                android:left="@dimen/dp_5"
                android:right="@dimen/dp_5"
                android:top="@dimen/dp_5" />
            <size
                android:width="@dimen/dp_30"
                android:height="@dimen/dp_30" />
            <solid android:color="@color/seekBarThumbOut" />
        </shape>
    </item>

    <item>
        <shape android:shape="oval">
            <size
                android:width="@dimen/dp_20"
                android:height="@dimen/dp_20" />
            <solid android:color="@color/seekBarThumbInner" />
        </shape>
    </item>
</layer-list>